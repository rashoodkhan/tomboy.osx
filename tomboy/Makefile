$(ASSEMBLY_MDB): $(ASSEMBLY)
$(ASSEMBLY): $(build_sources) $(build_resources) $(build_datafiles) $(DLL_REFERENCES) $(PROJECT_REFERENCES) $(build_xamlg_list) $(build_satellite_assembly_list)
	make pre-all-local-hook prefix=$(prefix)
	mkdir -p $(shell dirname $(ASSEMBLY))
	make $(CONFIG)_BeforeBuild
	$(ASSEMBLY_COMPILER_COMMAND) $(ASSEMBLY_COMPILER_FLAGS) -out:$(ASSEMBLY) -target:$(COMPILE_TARGET) $(build_sources_embed) $(build_resources_embed) $(build_references_ref)
	make $(CONFIG)_AfterBuild
	make post-all-local-hook prefix=$(prefix)

install-local: $(ASSEMBLY) $(ASSEMBLY_MDB)
	make pre-install-local-hook prefix=$(prefix)
	make install-satellite-assemblies prefix=$(prefix)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)'
	$(call cp,$(ASSEMBLY),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(ASSEMBLY_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(TOMBOY_EXE_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(INFO_PLIST),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(TOMBOY_LIBRARY_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(TOMBOY_LIBRARY_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(SERVICESTACK_COMMON_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(SERVICESTACK_INTERFACES_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(SERVICESTACK_ORMLITE_SQLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(SERVICESTACK_ORMLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(MONO_DATA_SQLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(DEVDEFINED_OAUTH_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(DEVDEFINED_OAUTH_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(MONOMAC_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	mkdir -p '$(DESTDIR)$(bindir)'
	$(call cp,$(TOMBOY),$(DESTDIR)$(bindir))
	make post-install-local-hook prefix=$(prefix)

uninstall-local: $(ASSEMBLY) $(ASSEMBLY_MDB)
	make pre-uninstall-local-hook prefix=$(prefix)
	make uninstall-satellite-assemblies prefix=$(prefix)
	$(call rm,$(ASSEMBLY),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(ASSEMBLY_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(TOMBOY_EXE_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(INFO_PLIST),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(TOMBOY_LIBRARY_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(TOMBOY_LIBRARY_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(SERVICESTACK_COMMON_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(SERVICESTACK_INTERFACES_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(SERVICESTACK_ORMLITE_SQLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(SERVICESTACK_ORMLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(MONO_DATA_SQLITE_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(DEVDEFINED_OAUTH_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(DEVDEFINED_OAUTH_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(MONOMAC_DLL),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call rm,$(TOMBOY),$(DESTDIR)$(bindir))
	make post-uninstall-local-hook prefix=$(prefix)

install: install-local
